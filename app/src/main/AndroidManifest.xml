<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kutyusearch">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.KutyuSearch">
        <activity
            android:name=".ui.about.About"
            android:label="@string/title_activity_about"
            android:theme="@style/Theme.KutyuSearch.NoActionBar"></activity>
        <activity
            android:name=".ui.description.DogDescription"
            android:label="@string/title_activity_dog_description"
            android:theme="@style/Theme.KutyuSearch.NoActionBar" />
        <activity
            android:name=".ui.breads.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.KutyuSearch.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>